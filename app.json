{
  "expo": {
    "name": "puzzle-pharm-app",
    "slug": "puzzle-pharm-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/P-logo.png",
    "scheme": "puzzlepharmapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/P-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.puzzlepharm.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/P-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/P-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fa6b85be-b37c-4d8f-b267-415fbaa8b799"
      }
    }
  }
}
